{
  "name": "alexandrie",
  "version": "1.0.0",
  "description": "A workflow to handle the administration and archiving of photos took at an event",
  "main": "src/index.ts",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "tslint --project tsconfig.json --type-check --force",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ssirois/alexandrie.git"
  },
  "author": "Samuel D. Sirois",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/ssirois/alexandrie/issues"
  },
  "homepage": "https://github.com/ssirois/alexandrie#readme",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn"
      },
      "electronWinstallerConfig": {
        "name": "electron_forge_alexandrie"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "electronforgealexandrie"
      }
    }
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.0.2",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "4.0.0",
    "jest": "^24.7.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.4.4"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "electron": "^4.1.4",
    "electron-compile": "^6.4.4",
    "electron-devtools-installer": "^2.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.4",
    "tslib": "^1.9.3"
  }
}
